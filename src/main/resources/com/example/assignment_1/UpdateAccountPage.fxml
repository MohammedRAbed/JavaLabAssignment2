<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="504.0" prefWidth="459.0" style="-fx-background-color: #e1e8f2;"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.assignment_1.Controller.UpdateAccountPageController">
    <children>
        <AnchorPane layoutX="7.0" layoutY="6.0" prefHeight="488.0" prefWidth="444.0"
                    style="-fx-background-color: white; -fx-background-radius: 40px;">
            <children>
                <Label layoutX="132.0" layoutY="14.0" prefHeight="46.0" prefWidth="210.0" text="Update User"
                       textFill="#3f4c6f">
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                    <padding>
                        <Insets left="30.0"/>
                    </padding>
                </Label>
                <Label layoutX="72.0" layoutY="120.0" prefHeight="46.0" prefWidth="143.0" text="User id :">
                    <font>
                        <Font name="System Bold" size="18.0"/>
                    </font>
                </Label>
                <Label layoutX="72.0" layoutY="166.0" prefHeight="46.0" prefWidth="143.0" text="Account No :">
                    <font>
                        <Font name="System Bold" size="18.0"/>
                    </font>
                </Label>
                <Label layoutX="72.0" layoutY="212.0" prefHeight="46.0" prefWidth="143.0" text="Username :">
                    <font>
                        <Font name="System Bold" size="18.0"/>
                    </font>
                </Label>
                <TextField fx:id="useridTF" layoutX="188.0" layoutY="124.0" styleClass="textField"
                           stylesheets="@../../../admin.css"/>
                <TextField fx:id="accountNoTF" layoutX="188.0" layoutY="170.0" styleClass="textField"
                           stylesheets="@../../../admin.css"/>
                <TextField fx:id="usernameTF" layoutX="188.0" layoutY="216.0" styleClass="textField"
                           stylesheets="@../../../admin.css"/>
                <Button fx:id="updateAccountBtn" layoutX="108.0" layoutY="411.0" mnemonicParsing="false"
                        onAction="#updateAccount" prefHeight="25.0" prefWidth="119.0" styleClass="saveButton"
                        stylesheets="@../../../admin.css" text="Save" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="18.0"/>
                    </font>
                </Button>
                <Button fx:id="cancelBtn" layoutX="233.0" layoutY="411.0" mnemonicParsing="false"
                        onAction="#cancelCreation" prefHeight="25.0" prefWidth="119.0" styleClass="cancelButton"
                        stylesheets="@../../../admin.css" text="Cancel" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="18.0"/>
                    </font>
                </Button>
                <TextField fx:id="currencyTF" layoutX="188.0" layoutY="262.0" styleClass="textField"
                           stylesheets="@../../../admin.css"/>
                <Label layoutX="72.0" layoutY="258.0" prefHeight="46.0" prefWidth="143.0" text="Currency :">
                    <font>
                        <Font name="System Bold" size="18.0"/>
                    </font>
                </Label>
                <Label layoutX="72.0" layoutY="304.0" prefHeight="46.0" prefWidth="143.0" text="Balance :">
                    <font>
                        <Font name="System Bold" size="18.0"/>
                    </font>
                </Label>
                <TextField fx:id="balanceTF" layoutX="188.0" layoutY="308.0" styleClass="textField"
                           stylesheets="@../../../admin.css"/>
                <Label layoutX="72.0" layoutY="350.0" prefHeight="46.0" prefWidth="143.0" text="Date :">
                    <font>
                        <Font name="System Bold" size="18.0"/>
                    </font>
                </Label>
                <TextField fx:id="dateTF" layoutX="188.0" layoutY="354.0" styleClass="textField"
                           stylesheets="@../../../admin.css"/>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>

